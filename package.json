{
  "name": "inner-circle-auth",
  "version": "0.0.1",
  "private": true,
  "homepage": "/auth",
  "scripts": {
    "start": "run-s create-config:dev start-devserver",
    "start:local": "run-s create-config:local start-devserver",
    "start-devserver": "react-scripts start",
    "build": "cross-env DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "release": "standard-version",
    "lint": "run-s lint:js lint:ts lint:css",
    "lint:ts": "eslint --no-error-on-unmatched-pattern ./src/**/*.{ts,tsx}",
    "lint:ts:fix": "eslint --no-error-on-unmatched-pattern ./src/**/*.{ts,tsx} --fix",
    "lint:js": "eslint --no-error-on-unmatched-pattern ./src/**/*.{js,jsx}",
    "lint:js:fix": "eslint --no-error-on-unmatched-pattern ./src/**/*.{js,jsx} --fix",
    "lint:css": "npx stylelint ./src/**/*.css",
    "lint:css:fix": "npx stylelint ./src/**/*.{css,scss} --fix",
    "create-config:dev": "node local-config-builder dev",
    "create-config:local": "node local-config-builder local",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tourmalinecore/react-tc-auth": "^2.2.6",
    "axios": "^1.2.2",
    "clsx": "^1.2.1",
    "history": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.5.0",
    "sass": "^1.53.0",
  },
  "devDependencies": {
    "@types/history": "^5.0.0",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "npm-run-all": "^4.1.5",
    "postcss-scss": "4.0.4",
    "standard-version": "^9.3.2",
    "stylelint": "^14.5.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "^5.9.3",
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  }
}
